{
	"name": "globalese/jira-php-client",
	"description": "PHP client for Jira",
	"authors": [
		{
			"name": "Greg Horváth"
		},
		{
			"name": "Száraz Richárd"
		}
	],
	"keywords": ["jira"],
	"license": "MIT",
	"type": "library",
	"autoload": {
		"psr-4": {
			"Scaramuccio\\Jira\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Scaramuccio\\Jira\\Test\\": "tests/"
		}
	},
	"require": {
		"ext-json": "*",
		"guzzlehttp/guzzle": "^7.4",
		"php": ">=7.3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"phpspec/prophecy-phpunit": "^2.0"
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	}
}
